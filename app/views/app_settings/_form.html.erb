
<%= simple_form_for(@app_setting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tab_name %>

    <%= f.input :theme_name, collection: AppSetting::APP_THEME, include_blank: 'select a theme' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
